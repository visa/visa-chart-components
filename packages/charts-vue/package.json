{
  "name": "@visa/charts-vue",
  "version": "1.1.0",
  "description": "A vue wrapper built with Stencil's bindings for the visa-charts-lib stencil.js based component library. ",
  "license": "SEE LICENCE IN LICENSE",
  "author": "Visa Data Experience Team",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visa/visa-chart-components.git",
    "directory": "/packages/charts-vue"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "audit": "yarn audit --json --level low > yarn-audit.json",
    "yarn-install": "yarn",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p . --outDir ./dist",
    "test:es5-check": "es-check es5 'dist/*.js'"
  },
  "dependencies": {
    "@visa/charts": "^7.2.0",
    "@visa/charts-types": "^0.6.1"
  },
  "devDependencies": {
    "@types/node": "18.11.19",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^4.4.4",
    "vue": "3"
  },
  "resolutions": {
    "minimist": "^1.2.6",
    "path-parse": "^1.0.7",
    "d3-color": "^3.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
